{
  "version": 2,
  "controller": {},
  "workers": [{
    "type": "router",
    "realms": [{
      "name": "v1",
      "roles": [{
        "name": "anonymous",
        "permissions": [{
          "uri": "*",
          "allow": {
            "call": true,
            "register": true,
            "publish": true,
            "subscribe": true
          }
        },
        {
          "uri": "wamp.*",
          "allow": {
            "call": false,
            "register": false,
            "publish": false,
            "subscribe": false
          }
        }]
      },
      {
        "name": "provider",
        "permissions": [{
          "uri": "*",
          "allow": {
            "call": true,
            "register": true,
            "publish": true,
            "subscribe": true
          }
        },
        {
          "uri": "wamp.*",
          "allow": {
            "call": true,
            "register": false,
            "publish": true,
            "subscribe": true
          }
        }]
      }]
    }],
    "transports": [{
      "type": "web",
      "endpoint": {
        "type": "tcp",
        "port": 41314
      },
      "paths": {
        "ws": {
          "type": "websocket",
          "auth": {
            "ticket": {
              "type": "static",
              "principals": {
                "kadecot-provider": {
                  "ticket": "KADECOT_PROVIDER",
                  "role": "provider"
                }
              }
            },
            "anonymous": {
              "type": "static",
              "role": "anonymous"
            }
          }
        }
      }
    }]
  }]
}
